{
    optional uint64 account_uid        		= 1;
    optional uint32 channel_id       		= 2;
    optional uint64 role_uid     			= 3;
    optional string mobile_uuid    			= 4;
    optional uint32 source_type     		= 5;
    optional uint32 log_time        		= 6;
}

